{
  "name": "_blank",
  "version": "1.0.0",
  "description": "Yet another React-based web application starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/froloff/_blank.git"
  },
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node server/index.js",
    "build": "rimraf build && cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "eslint": "eslint --cache --ext .jsx,.js --ignore-path .gitignore ./",
    "stylelint": "stylelint client/**/*.pcss --cache",
    "lint": "yarn eslint && yarn stylelint",
    "precommit": "yarn lint"
  },
  "author": "Leonid Frolov <leonid@froloff.me> (https://github.com/froloff)",
  "license": "CC0-1.0",
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "axios": "^0.21.0",
    "babel-loader": "^8.2.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "express": "^4.17.3",
    "file-loader": "^6.2.0",
    "json-proxy": "^0.9.3",
    "mini-css-extract-plugin": "^1.3.1",
    "normalize.css": "^8.0.1",
    "postcss": "^8.1.14",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-nested": "^5.0.2",
    "postcss-reporter": "^7.0.2",
    "postcss-url": "^10.1.1",
    "prop-types": "^15.7.2",
    "pug": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
